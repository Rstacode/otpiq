{
    "name": "rstacode/otpiq",
    "description": "A Laravel package for handling OTP verification, The most reliable SMS & WhatsApp & Telegram verification platform for your business in Iraq",
    "keywords": [
        "laravel",
        "otp",
        "sms",
        "whatsapp",
        "telegram",
        "verification",
        "iraq",
        "kurdistan"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rstacode",
            "email": "rstacode@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/rstacode/otpiq",
    "autoload": {
        "psr-4": {
            "Rstacode\\Otpiq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rstacode\\Otpiq\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
  "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rstacode\\Otpiq\\OtpiqServiceProvider"
            ],
            "aliases": {
                "Otpiq": "Rstacode\\Otpiq\\Facades\\Otpiq"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "support": {
        "issues": "https://github.com/rstacode/otpiq/issues",
        "source": "https://github.com/rstacode/otpiq"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}